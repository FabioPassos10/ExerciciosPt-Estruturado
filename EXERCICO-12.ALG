Algoritmo "Exercico-12"

// Autor(a)    : Lavínia Bueno
// Data atual  : 15/03/2022
Var
  n1,n2,n3,T:real
Inicio
  Escreva("Digite o valor de N1: ")
  Leia(n1)
  Escreva("Digite o valor de N2: ")
  Leia(n2)
  Escreva("Digite o valor de N3: ")
  Leia(n3)
  T := (n1 *(n2 + n3))/2
  Escreva("O resultado é: ",T)

Fimalgoritmo